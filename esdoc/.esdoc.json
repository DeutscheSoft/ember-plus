{
  "source": "../src",
  "index": "../README.md",
  "destination": "./out",
  "package": "../package.json",
  "plugins": [
    {
      "name": "esdoc-brand-plugin",
      "option": {
        "title": "ember-plus",
        "description": "A Ember+ JavaScript client library",
        "repository": "https://github.com/deutschesoft/ember-plus",
        "site": "docs.deuso.de/ember-plus",
        "author": "Arne Goedeke"
      }
    },
    {
      "name": "esdoc-inject-style-plugin",
      "option": {
      "enable": true,
        "styles": [
        ]
      }
    },
    {
      "name": "esdoc-publish-html-plugin"
    },
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "coverage": {"enable": true},
        "accessor": {"access": ["public", "protected" ], "autoPrivate": true}
      }
    }
  ]
}
